<h1>New Wiki</h1>

<div class="row">
  <div class="col-md-8">
    <%= form_for @wiki do |f| %>
    <div class="form-group">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div class="form-group">
      <%= f.label :body %>
      <%= f.text_area :body %>
    </div>
    <div class="form-group">
      <%= f.label :private %>
      <%= f.check_box(:private) %>
    <div class="form-group">
      <%= f.submit "Save" %>
    </div>
  <% end %>
  </div>
</div>